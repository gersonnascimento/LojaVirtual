<html>
<%= form_for Produto.new do |f| %>
<%= f.label :nome %>   
<%= f.text_field :nome , value:@p.nome%>
<%= f.label :valor %>   
<%= f.text_field :valor, value:@p.valor%>
<%= f.label :quantidade %>   
<%= f.text_field :quantidade, value:@p.quantidade %>
<%= f.submit "Salvar Alterações" %>
<% end %>
</html>